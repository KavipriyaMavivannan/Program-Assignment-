package.com.kavipriya;
import java.util...;
public class Fibonacci {
    public static void main(String args[]){
       Scanner s=new Scanner(System.in);
       System. out.print("Enter no. Of terms:");
       int n=s.nextInt() ;
       int i,k1=0,k2=1,sum=0;
       for(i=1;i<=n;i++){
         System.out.print(k1+"+");
         sum=k1+k2;
         k1=k2;
         k2=sum;
        }
        System.out.print(â€œ\n Sum is"+sum);
    }
}
